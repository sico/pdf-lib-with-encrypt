{
  "name": "pdf-lib-with-encrypt",
  "version": "1.1.8",
  "description": "Create and modify PDF files with JavaScript",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./cjs/index.d.ts",
  "main": "./cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "test": "jest --config jest.json --runInBand",
    "build": "yarn run clean && yarn build:cjs && yarn build:esm",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json ",
    "apps:web": "http-server -c-1 .",
    "apps:web:mac": "bash -c 'sleep 1 && open http://localhost:8080/apps/web/test1.html' & yarn apps:web"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sico/pdf-lib-with-encrypt.git"
  },
  "keywords": [
    "pdf-lib",
    "pdf",
    "document",
    "create",
    "modify",
    "creation",
    "modification",
    "edit",
    "editing",
    "typescript",
    "javascript",
    "library"
  ],
  "author": "John Sico",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sico/pdf-lib-with-encrypt/issues"
  },
  "dependencies": {
    "@pdf-lib/standard-fonts": "^1.0.0",
    "@pdf-lib/upng": "^1.0.1",
    "@types/crypto-js": "^4.0.2",
    "crypto-js": "^4.0.0",
    "pako": "^1.0.11",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@ovos-media/ts-transform-paths": "^1.7.18-1",
    "@pdf-lib/fontkit": "^1.1.0",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@types/jest": "^26.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/pako": "^2.0.0",
    "http-server": "^0.12.3",
    "jest": "^26.0.1",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "rollup": "^2.78.1",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  }
}
